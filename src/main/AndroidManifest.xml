<?xml version="1.0" encoding="UTF-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.example.autotest">
     <uses-permission android:name="android.permission.WAKE_LOCK" />
     <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />
     <uses-permission android:name="android.permission.BLUETOOTH" />
     <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
     <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
     <application android:allowBackup="true" android:icon="@mipmap/ic_launcher" android:name="com.mdx.framework.MApplication" android:label="@string/app_name" android:supportsRtl="true" android:theme="@style/AppTheme">
          <activity android:name=".MainActivity" android:label="@string/app_name" android:theme="@style/AppTheme.NoActionBar">
   
          </activity>
          <activity android:name="com.example.autotest.act.ActUpdateself" android:screenOrientation="portrait" />
          <activity android:name="com.example.autotest.act.ActMain" android:screenOrientation="portrait" >
	    <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
	  </activity>
     </application>
</manifest>

